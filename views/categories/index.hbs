<section class="articles">
  <h1>{{ __ "categories" }}</h1>
  <ul>
    {{#each categories }}
    <li>
      <a href="{{ this.url }}/edit">
        <header>
          <h4>{{ this.name }}</h4>
          <div class="category">{{ this.count }}</div>
          <time datetime="{{ formatDate this.createdAt showTime=true }}">{{ formatDate this.createdAt}}</time>
        </header>
      </a>
    </li>
    {{/each}}
  </ul>
</section>
<pre class="debug">{{ json categories }}</pre>
